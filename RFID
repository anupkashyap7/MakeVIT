#include <SPI.h>
#include <RFID.h>
#define SDA_DIO 9
#define RESET_DIO 8
RFID RC522(SDA_DIO, RESET_DIO); 
String a="";

void setup()
{ 
  Serial.begin(9600);
  SPI.begin(); 
  RC522.init();
}

void loop()
{
  if (RC522.isCard())
  {
    String a="";
    RC522.readCardSerial();
    for(int i=0;i<5;i++)
    {
      a.concat(RC522.serNum[i]);
    }
    Serial.println(a);
    Serial.println();
  }
  delay(1000);
}
